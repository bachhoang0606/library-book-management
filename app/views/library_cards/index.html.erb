<p id="notice"><%= notice %></p>

<h1>Library Cards</h1>

<table>
  <thead>
    <tr>
      <th>Start date</th>
      <th>Expiration date</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @library_cards.each do |library_card| %>
      <tr>
        <td><%= library_card.start_date %></td>
        <td><%= library_card.expiration_date %></td>
        <td><%= library_card.note %></td>
        <td><%= link_to 'Show', library_card %></td>
        <td><%= link_to 'Edit', edit_library_card_path(library_card) %></td>
        <td><%= link_to 'Destroy', library_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Library Card', new_library_card_path %>
